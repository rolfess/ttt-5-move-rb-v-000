#!/usr/bin/env ruby

require_relative '../lib/move.rb'

puts "Welcome to Tic Tac Toe!"
board=[" ", " ", " "," ", " ", " "," ", " ", " ",]
char= "X"
display_board(board)
puts "Where would you like to go?"
input=gets.strip
puts input
input_to_index(input)
move(board, input, char)
display_board(board)
