#!/usr/bin/env ruby

require_relative '../lib/move.rb'

puts "Welcome to Tic Tac Toe!"
board=[" ", " ", " "," ", " ", " "," ", " ", " ",]
display_board(board)
puts "Where would you like to go?"
user_input=gets.chop
input_to_index(user_input)
index=user_input
move(board, index, char)
display_board(board)
